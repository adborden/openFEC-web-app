  <div class="toggles">
    {{#each this}}
    <label>
      <input
          type="radio"
          name="cycle-toggle"
          value="{{max}}"
          data-election-full="{{electionFull}}"
          {{#if checked}}checked{{/if}}
        />
      <span class="button button--neutral">
        {{#if electionFull}}
        Full cycle:
        {{/if}}
        {{min}}â€“{{max}}
      </span>
    </label>
    {{/each}}
